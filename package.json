{
  "name": "react-file-formatter",
  "version": "1.1.6",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rm -rf node_modules && npm cache clean --force && rm -rf package-lock.json && rm -rf dist && npm install",
    "run": "npm pack --dry-run",
    "pub": "npm publish"
  },
  "bin": {
    "react-file-formatter": "src/index.js"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "files": [
    "README.md",
    "package.json",
    "src/index.js",
    "utils"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/krishzuz/react-file-formatter"
  },
  "keywords": [
    "react",
    "formatter",
    "cli",
    "react-query"
  ],
  "author": "krishzuz",
  "license": "MIT",
  "description": "react-file-formatter is a command-line tool to format and standardize React code files. It helps you clean up and format your React files with ease.",
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@babel/generator": "^7.26.2",
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.25.9"
  }
}

{
  "name": "react-file-formatter",
  "version": "1.1.6",
  "main": "./src/index.js",
  "scripts": {
    "clean": "rm -rf node_modules && npm cache clean --force && rm -rf package-lock.json && rm -rf dist && npm install",
    "dry": "npm pack --dry-run",
    "publish": "npm publish",
    "release": "npm pack",
    "format": "prettier --write .",
    "lint": "prettier --check .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm pack"
  },
  "bin": {
    "react-file-formatter": "src/index.js"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "files": [
    "README.md",
    "package.json",
    "src/index.js",
    "utils"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/krishzuz/react-file-formatter.git"
  },
  "keywords": [
    "react",
    "formatter",
    "cli",
    "file-formatter",
    "formatting",
    "react-file-formatter",
    "format",
    "prettier"
  ],
  "author": "krishzuz",
  "license": "ISC",
  "description": "react-file-formatter is a command-line tool to format and standardize React code files. It helps you clean up and format your React files with ease.",
  "devDependencies": {
    "prettier": "^3.3.3"
  },
  "dependencies": {
    "@babel/generator": "^7.26.2",
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.25.9"
  }
}
